{
  "name": "zipper",
  "version": "0.0.0",
  "description": "Cloud Runner Job for zipping entries",
  "author": "cbolles @cbolles",
  "bin": {
    "zipper": "./bin/run"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "cbolles/hello-world",
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "adm-zip": "^0.5.12",
    "archiver": "^7.0.1"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/adm-zip": "^0.5.5",
    "@types/archiver": "^7.0.0",
    "@types/chai": "^4",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8.57.0",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "oclif": {
    "bin": "zipper",
    "dirname": "zipper",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "prettier": "prettier -l \"src/**/*.ts\"",
    "prettier:fix": "prettier -wl \"src/**/*.ts\"",
    "postpack": "shx rm -f oclif.manifest.json",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "bugs": "https://github.com/cbolles/hello-world/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
